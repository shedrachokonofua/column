{
  "name": "column",
  "version": "1.0.0",
  "description": "Job Lead Generator",
  "main": "index.js",
  "dependencies": {
    "ajv": "^6.11.0"
  },
  "devDependencies": {
    "@ava/babel": "^1.0.0",
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/register": "^7.8.3",
    "ava": "^3.0.0",
    "eslint-plugin-ava": "^10.0.0",
    "husky": "^4.2.1",
    "xo": "^0.25.3"
  },
  "scripts": {
    "test": "ava",
    "lint": "xo",
    "lint:fix": "xo --fix",
    "build:exec": "babel src -d build",
    "build": "npm run lint && npm test && npm run build:exec"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shedrachokonofua/column.git"
  },
  "author": "Shedrach Okonofua",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/shedrachokonofua/column/issues"
  },
  "homepage": "https://github.com/shedrachokonofua/column#readme",
  "husky": {
    "pre-commit": "npm run lint:fix",
    "pre-push": "npm test"
  },
  "ava": {
    "babel": true,
    "require": [
      "./test/helpers/register-source-files.js"
    ],
    "files": [
      "!build"
    ]
  },
  "xo": {
    "space": true,
    "extends": "plugin:ava/recommended"
  }
}
